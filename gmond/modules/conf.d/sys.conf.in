modules {
  module {
    name = "sys_module"
    path = "@libdir@/ganglia/modsys.so" 
  }
}


/* This collection group will send general info about this host every 1200 secs. 
   This information doesn't change between reboots and is only collected once. */ 
collection_group { 
  collect_once = yes 
  time_threshold = 1200 
  metric { 
    name = "mboottime" 
  } 
  metric { 
    name = "mmachine_type" 
  } 
  metric { 
    name = "mos_name" 
  } 
  metric { 
    name = "mos_release" 
  } 
} 

