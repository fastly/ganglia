modules {
  module {
    name = "multicpu_module"
    path = "@libdir@/ganglia/modmulticpu.so" 
  }
}


#/* Multi CPU DSO metric */ 
collection_group { 
  collect_every = 10 
  time_threshold = 50 
  metric { 
    name = "multicpu_user0" 
    value_threshold = 1.0 
  } 
  metric { 
    name = "multicpu_nice0" 
    value_threshold = 1.0 
  } 
  metric { 
    name = "multicpu_system0" 
    value_threshold = 1.0 
  } 
  metric { 
    name = "multicpu_idle0" 
    value_threshold = 1.0 
  } 
  metric { 
    name = "multicpu_wio0" 
    value_threshold = 1.0 
  } 
  metric { 
    name = "multicpu_intr0" 
    value_threshold = 1.0 
  } 
  metric { 
    name = "multicpu_sintr0" 
    value_threshold = 1.0 
  } 
}


